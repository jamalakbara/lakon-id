<div class="page-width">
  {% if section.settings.logo_subtitle %}
    <figure class="custom-page-figure" style="text-align: center;">
      <img
        srcset="
          {%- if section.settings.logo_subtitle.width >= 165 -%}{{ section.settings.logo_subtitle | image_url: width: 165 }} 165w,{%- endif -%}
          {%- if section.settings.logo_subtitle.width >= 360 -%}{{ section.settings.logo_subtitle | image_url: width: 360 }} 360w,{%- endif -%}
          {%- if section.settings.logo_subtitle.width >= 533 -%}{{ section.settings.logo_subtitle | image_url: width: 533 }} 533w,{%- endif -%}
          {%- if section.settings.logo_subtitle.width >= 720 -%}{{ section.settings.logo_subtitle | image_url: width: 720 }} 720w,{%- endif -%}
          {%- if section.settings.logo_subtitle.width >= 940 -%}{{ section.settings.logo_subtitle | image_url: width: 940 }} 940w,{%- endif -%}
          {%- if section.settings.logo_subtitle.width >= 1066 -%}{{ section.settings.logo_subtitle | image_url: width: 1066 }} 1066w,{%- endif -%}
          {{ section.settings.logo_subtitle | image_url }} {{ section.settings.logo_subtitle.width }}w
        "
        src="{{ section.settings.logo_subtitle | image_url: width: 533 }}"
        {% comment %} sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)" {% endcomment %}
        alt="{{ section.settings.logo_subtitle.alt | escape }}"
        class="motion-reduce"
        {% unless lazy_load == false %}
          loading="lazy"
        {% endunless %}
        width="{{ section.settings.logo_subtitle.width }}"
        height="{{ section.settings.logo_subtitle.height }}"
      >
    </figure>
  {% else %}
    <h1 class="main-page-title page-title h0{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--fade-in{% endif %}" style="text-align: center;">
      {{ page.title | escape }}
    </h1>
  {% endif %}
  

  <h2 class="page-title {{ section.settings.heading_size }}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}" style="text-align: center;">
    {%- if section.settings.subtitle != blank -%}
      {{ section.settings.subtitle | escape }}
    {%- endif -%}
  </h2>

  <div class="rte{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}" style="text-align: center;">
    {%- if section.settings.content != blank -%}
      {{ section.settings.content }}
    {%- endif -%}
  </div>

  {% if section.settings.image %}
    <figure class="custom-page-figure">
      <img
        srcset="
          {%- if section.settings.image.width >= 165 -%}{{ section.settings.image | image_url: width: 165 }} 165w,{%- endif -%}
          {%- if section.settings.image.width >= 360 -%}{{ section.settings.image | image_url: width: 360 }} 360w,{%- endif -%}
          {%- if section.settings.image.width >= 533 -%}{{ section.settings.image | image_url: width: 533 }} 533w,{%- endif -%}
          {%- if section.settings.image.width >= 720 -%}{{ section.settings.image | image_url: width: 720 }} 720w,{%- endif -%}
          {%- if section.settings.image.width >= 940 -%}{{ section.settings.image | image_url: width: 940 }} 940w,{%- endif -%}
          {%- if section.settings.image.width >= 1066 -%}{{ section.settings.image | image_url: width: 1066 }} 1066w,{%- endif -%}
          {{ section.settings.image | image_url }} {{ section.settings.image.width }}w
        "
        src="{{ section.settings.image | image_url: width: 533 }}"
        {% comment %} sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)" {% endcomment %}
        alt="{{ section.settings.image.alt | escape }}"
        class="motion-reduce"
        {% unless lazy_load == false %}
          loading="lazy"
        {% endunless %}
        width="{{ section.settings.image.width }}"
        height="{{ section.settings.image.height }}"
      >
    </figure>
  {% endif %}

  <div class="page-width page-width--narrow">
    <div class="rte{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
      {{ page.content }}
    </div>
  </div>

  <div class="page-width container-slide-page-custom">
    {% for block in section.blocks %}
      {% case block.type %}
        {% when 'collections' %}
          <div class="collection-slide-dude">
            <div class="slide-page-custom">
              {% if block.settings.image1 %}
                <figure class="custom-page-figure">
                  <img
                    srcset="
                      {%- if block.settings.image1.width >= 165 -%}{{ block.settings.image1 | image_url: width: 165 }} 165w,{%- endif -%}
                      {%- if block.settings.image1.width >= 360 -%}{{ block.settings.image1 | image_url: width: 360 }} 360w,{%- endif -%}
                      {%- if block.settings.image1.width >= 533 -%}{{ block.settings.image1 | image_url: width: 533 }} 533w,{%- endif -%}
                      {%- if block.settings.image1.width >= 720 -%}{{ block.settings.image1 | image_url: width: 720 }} 720w,{%- endif -%}
                      {%- if block.settings.image1.width >= 940 -%}{{ block.settings.image1 | image_url: width: 940 }} 940w,{%- endif -%}
                      {%- if block.settings.image1.width >= 1066 -%}{{ block.settings.image1 | image_url: width: 1066 }} 1066w,{%- endif -%}
                      {{ block.settings.image1 | image_url }} {{ block.settings.image1.width }}w
                    "
                    src="{{ block.settings.image1 | image_url: width: 533 }}"
                    {% comment %} sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)" {% endcomment %}
                    alt="{{ block.settings.image1.alt | escape }}"
                    class="motion-reduce"
                    {% unless lazy_load == false %}
                      loading="lazy"
                    {% endunless %}
                    width="{{ block.settings.image1.width }}"
                    height="{{ block.settings.image1.height }}"
                  >
                </figure>
              {% endif %}
      
              {% if block.settings.image2 %}
                <figure class="custom-page-figure">
                  <img
                    srcset="
                      {%- if block.settings.image2.width >= 165 -%}{{ block.settings.image2 | image_url: width: 165 }} 165w,{%- endif -%}
                      {%- if block.settings.image2.width >= 360 -%}{{ block.settings.image2 | image_url: width: 360 }} 360w,{%- endif -%}
                      {%- if block.settings.image2.width >= 533 -%}{{ block.settings.image2 | image_url: width: 533 }} 533w,{%- endif -%}
                      {%- if block.settings.image2.width >= 720 -%}{{ block.settings.image2 | image_url: width: 720 }} 720w,{%- endif -%}
                      {%- if block.settings.image2.width >= 940 -%}{{ block.settings.image2 | image_url: width: 940 }} 940w,{%- endif -%}
                      {%- if block.settings.image2.width >= 1066 -%}{{ block.settings.image2 | image_url: width: 1066 }} 1066w,{%- endif -%}
                      {{ block.settings.image2 | image_url }} {{ block.settings.image2.width }}w
                    "
                    src="{{ block.settings.image2 | image_url: width: 533 }}"
                    {% comment %} sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)" {% endcomment %}
                    alt="{{ block.settings.image2.alt | escape }}"
                    class="motion-reduce"
                    {% unless lazy_load == false %}
                      loading="lazy"
                    {% endunless %}
                    width="{{ block.settings.image2.width }}"
                    height="{{ block.settings.image2.height }}"
                  >
                </figure>
              {% endif %}
      
              {% if block.settings.image3 %}
                <figure class="custom-page-figure">
                  <img
                    srcset="
                      {%- if block.settings.image3.width >= 165 -%}{{ block.settings.image3 | image_url: width: 165 }} 165w,{%- endif -%}
                      {%- if block.settings.image3.width >= 360 -%}{{ block.settings.image3 | image_url: width: 360 }} 360w,{%- endif -%}
                      {%- if block.settings.image3.width >= 533 -%}{{ block.settings.image3 | image_url: width: 533 }} 533w,{%- endif -%}
                      {%- if block.settings.image3.width >= 720 -%}{{ block.settings.image3 | image_url: width: 720 }} 720w,{%- endif -%}
                      {%- if block.settings.image3.width >= 940 -%}{{ block.settings.image3 | image_url: width: 940 }} 940w,{%- endif -%}
                      {%- if block.settings.image3.width >= 1066 -%}{{ block.settings.image3 | image_url: width: 1066 }} 1066w,{%- endif -%}
                      {{ block.settings.image3 | image_url }} {{ block.settings.image3.width }}w
                    "
                    src="{{ block.settings.image3 | image_url: width: 533 }}"
                    {% comment %} sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)" {% endcomment %}
                    alt="{{ block.settings.image3.alt | escape }}"
                    class="motion-reduce"
                    {% unless lazy_load == false %}
                      loading="lazy"
                    {% endunless %}
                    width="{{ block.settings.image3.width }}"
                    height="{{ block.settings.image3.height }}"
                  >
                </figure>
              {% endif %}
      
              {% if block.settings.image4 %}
                <figure class="custom-page-figure">
                  <img
                    srcset="
                      {%- if block.settings.image4.width >= 165 -%}{{ block.settings.image4 | image_url: width: 165 }} 165w,{%- endif -%}
                      {%- if block.settings.image4.width >= 360 -%}{{ block.settings.image4 | image_url: width: 360 }} 360w,{%- endif -%}
                      {%- if block.settings.image4.width >= 533 -%}{{ block.settings.image4 | image_url: width: 533 }} 533w,{%- endif -%}
                      {%- if block.settings.image4.width >= 720 -%}{{ block.settings.image4 | image_url: width: 720 }} 720w,{%- endif -%}
                      {%- if block.settings.image4.width >= 940 -%}{{ block.settings.image4 | image_url: width: 940 }} 940w,{%- endif -%}
                      {%- if block.settings.image4.width >= 1066 -%}{{ block.settings.image4 | image_url: width: 1066 }} 1066w,{%- endif -%}
                      {{ block.settings.image4 | image_url }} {{ block.settings.image4.width }}w
                    "
                    src="{{ block.settings.image4 | image_url: width: 533 }}"
                    {% comment %} sizes="(min-width: {{ settings.page_width }}px) {{ settings.page_width | minus: 130 | divided_by: 4 }}px, (min-width: 990px) calc((100vw - 130px) / 4), (min-width: 750px) calc((100vw - 120px) / 3), calc((100vw - 35px) / 2)" {% endcomment %}
                    alt="{{ block.settings.image4.alt | escape }}"
                    class="motion-reduce"
                    {% unless lazy_load == false %}
                      loading="lazy"
                    {% endunless %}
                    width="{{ block.settings.image4.width }}"
                    height="{{ block.settings.image4.height }}"
                  >
                </figure>
              {% endif %}
            </div>
            <div class="collection-slide-dude__content">
              <h2 class="page-title {{ section.settings.heading_size }}{% if settings.animations_reveal_on_scroll %} scroll-trigger animate--slide-in{% endif %}">
                {%- if block.settings.title != blank -%}
                  {{ block.settings.title | escape }}
                {%- endif -%}
              </h2>
            </div>
          </div>
      {% endcase %}
    {% endfor %}
  </div>
  
  {% for block in section.blocks %}
    {% case block.type %}
      {% when 'page' %}
        {{ block.settings.page.content }}
    {% endcase %}
  {% endfor %}

</div>

{% schema %}
{
  "name": "Custom Page",
  "tag": "section",
  "class": "section",
  "settings": [
    {
      "type": "header",
      "content": "Padding"
    },
    {
      "type": "range",
      "id": "padding_top",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Padding Top",
      "default": 36
    },
    {
      "type": "range",
      "id": "padding_bottom",
      "min": 0,
      "max": 100,
      "step": 4,
      "unit": "px",
      "label": "Padding Bottom",
      "default": 36
    },
    {
      "type": "header",
      "content": "Subtitle"
    },
    {
      "type": "text",
      "id": "subtitle",
      "label": "Subtitle"
    },
    {
      "type": "header",
      "content": "Logo Subtitle"
    },
    {
      "type": "image_picker",
      "id": "logo_subtitle",
      "label": "Logo Subtitle"
    },
    {
      "type": "header",
      "content": "Content"
    },
    {
      "type": "richtext",
      "id": "content",
      "label": "Content"
    },
    {
      "type": "header",
      "content": "Image"
    },
    {
      "type": "image_picker",
      "id": "image",
      "label": "Image"
    }
  ],
  "blocks": [
    {
      "type": "collections",
      "name": "Collections",
      "settings": [
        {
          "type": "header",
          "content": "Collection title"
        },
        {
          "type": "text",
          "id": "title",
          "label": "Title"
        },
        {
          "type": "header",
          "content": "Image Collection"
        },
        {
          "type": "image_picker",
          "id": "image1",
          "label": "Image 1"
        },
        {
          "type": "image_picker",
          "id": "image2",
          "label": "Image 2"
        },
        {
          "type": "image_picker",
          "id": "image3",
          "label": "Image 3"
        },
        {
          "type": "image_picker",
          "id": "image4",
          "label": "Image 4"
        }
      ]
    },
    {
      "type": "page",
      "name": "Add More Block",
      "settings": [
        {
          "type": "page",
          "id": "page",
          "label": "t:sections.main-product.blocks.collapsible_tab.settings.page.label"
        } 
      ]
    },
  ]
}
{% endschema %}